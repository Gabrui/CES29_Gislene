<div class="header">
  <div class="container">
    <h1>Adaptative Object Model</h1>
  </div>
</div>

<div class="messages">
  <div class="container">
  
    <% @tipos.each do |tipo| %> 
    <div class="message"> 
      <p class="content"><%= tipo.nome_tipo %></p> 
      <p class="time"><%= tipo.created_at %></p> 
    </div> 
    <% end %>
    <%= link_to 'Criar Tipo', "aom/novo" %>
  </div>
</div>


